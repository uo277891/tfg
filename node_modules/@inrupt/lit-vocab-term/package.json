{
  "name": "@inrupt/lit-vocab-term",
  "version": "0.3.5",
  "description": "Very basic implementation of LIT Vocab Term, with minimal RDF library dependencies.",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/inrupt/lit-vocab-term-js.git",
    "directory": "lit-vocab-term/lit-vocab-term-base"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint --config .eslintrc.js --fix \"src/**\"",
    "test": "eslint --config .eslintrc.js \"src/**\" && jest --no-cache ./src",
    "preversion": "npm test",
    "build": "rollup --config rollup.config.js",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@rdfjs/data-model": "1.1.2",
    "@types/rdf-js": "3.0.0"
  },
  "author": {
    "name": "Pat McBennett"
  },
  "contributors": [
    {
      "name": "Nicolas Seydoux"
    }
  ],
  "license": "Proprietary",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@types/chai": "^4.2.11",
    "@types/jest": "^25.2.1",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "chai": "^4.2.0",
    "chai-string": "^1.5.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-license-header": "^0.2.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^25.4.0",
    "lint-staged": "^10.1.7",
    "prettier": "2.0.5",
    "rollup": "^2.7.3",
    "rollup-plugin-typescript2": "^0.27.0",
    "ts-jest": "^25.4.0",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
